// opgave1c.cpp : This file contains the 'main' function. Program execution begins and ends there.

#include <stdio.h>

//Mathias Vestergaard (s211933) 29/8 - 2022
// I denne opgave skal der udføres arimetiske operationer med modulus for at undersøge om hvorvidt tallet er et heltal og om det er deleligt med 5

int main()
{
    
    int a;

    printf_s("Indtast et heltal som skal deles med 5\n");
    scanf_s("%d", &a);

    //Vi undersøger om a er et heltal
    if (a % 2 == 0) {

       // printf_s("\n %d er et heltal\n", a);
       // 
         //Vi undersøger om a er deleligt med 5
        if (a % 5 == 0) {

            printf_s("\n%d er deleligt med 5\n", a);
        }

        //Vi undersøger om a ikke er deleligt med 5
        if (a % 5 != 0) {

            printf_s("\n%d er ikke deleligt med 5", a);
        }
    }

    //Vi undersøger om a ikke er et heltal
    if (a % 2 != 0) {

        printf_s("\n %d er ikke et heltal\n", a);
    }

   
}

// Run program: Ctrl + F5 or Debug > Start Without Debugging menu
// Debug program: F5 or Debug > Start Debugging menu

